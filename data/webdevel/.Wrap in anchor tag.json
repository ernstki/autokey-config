{
    "type": "script",
    "filter": {
        "isRecursive": false,
        "regex": null
    },
    "showInTrayMenu": false,
    "store": {},
    "usageCount": 0,
    "prompt": false,
    "modes": [
        3
    ],
    "abbreviation": {
        "immediate": false,
        "backspace": true,
        "ignoreCase": false,
        "triggerInside": false,
        "wordChars": "[\\w]",
        "abbreviations": []
    },
    "description": "Wrap in anchor tag",
    "omitTrigger": false,
    "hotkey": {
        "hotKey": "a",
        "modifiers": [
            "<alt>",
            "<ctrl>",
            "<shift>"
        ]
    }
}