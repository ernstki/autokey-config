{
    "showInTrayMenu": false,
    "abbreviation": {
        "ignoreCase": false,
        "immediate": true,
        "backspace": true,
        "wordChars": "[\\w]",
        "abbreviations": [
            "<ttt"
        ],
        "triggerInside": true
    },
    "type": "script",
    "hotkey": {
        "hotKey": "t",
        "modifiers": [
            "<alt>",
            "<ctrl>",
            "<shift>"
        ]
    },
    "prompt": false,
    "modes": [
        1,
        3
    ],
    "omitTrigger": false,
    "filter": {
        "regex": null,
        "isRecursive": false
    },
    "usageCount": 40,
    "store": {},
    "description": "Typewriter"
}